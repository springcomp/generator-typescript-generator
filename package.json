{
  "name": "@springcomp/generator-typescript-generator",
  "version": "0.2.0",
  "description": "",
  "files": [
    "generators"
  ],
  "keywords": [
    "yeoman-generator",
    "typescript"
  ],
  "scripts": {
    "build": "npx tsc --project . --outDir ./dist/generators/",
    "clean": "shx rm -rf ./dist/",
    "package": "npm run build && npm run _boilerplate_files && npm run _template_files && npm run _clear-package-json",
    "_boilerplate_files": "shx cp LICENSE README.md ./dist/",
    "_clear-package-json": "shx cp package.json ./dist/",
    "_template_files": "npm run _app_static_files && npm run _app_template_files",
    "_app_template_files": "npx copyfiles -u 2 generators/app/templates/**/**/* ./dist/generators/app/",
    "_app_static_files": "npx copyfiles generators/**/**/*.ts ./dist/generators/app/templates/"
  },
  "dependencies": {
    "@springcomp/generator-ts": "^0.7.1",
    "chalk": "^5.0.0",
    "deep-extend": "^0.6.0",
    "inquirer-npm-name": "^4.0.0",
    "mkdirp": "^1.0.4",
    "yeoman-generator": "^4.2.0",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@types/inquirer": "^8.1.3",
    "@types/inquirer-npm-name": "^3.0.2",
    "@types/lodash": "^4.14.178",
    "@types/mkdirp": "^1.0.2",
    "@types/yeoman-generator": "^5.2.8",
    "clean-publish": "^3.4.4",
    "copyfiles": "^2.3.0",
    "shx": "^0.3.4",
    "typescript": "^4.5.4",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^6.2.0"
  }
}
